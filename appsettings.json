{
  "ConnectionStrings": {
    "AssistenteDatabase": "User ID=postgres;Password=postgres;Host=localhost;Port=1000;Database=assistente_db;"
  },
  "OpenAI": {
    "ApiKey": "<API_KEY>"
  },
  "Qdrant": {
    "Host": "localhost",
    "Port": 1003
  },
  "Rabbit": {
    "Name": "ASSISTENTE.Worker.Sync",
    "Url": "amqp://guest:guest@localhost:1004"
  },
  "InternalApi": {
    "Url": "http://localhost:1009"
  },
  "KnowledgePaths": {
    "MarkdownNotes": "<PATH_TO_MARKDOWN_NOTES_ROOT>",
    "Repositories": "<PATH_TO_REPOSITORIES_ROOT>"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:1007"
        }
      }
    ]
  }
}
